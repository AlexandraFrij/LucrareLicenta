<resources>
    <style name="CalendarDateText">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="CalendarWeekDayText">
        <item name="android:textColor">@color/fadcd5</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="CustomAlertDialog" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:background">@color/a4b2138</item>
        <item name="android:textColor">@color/fadcd5</item>
        <item name="android:textAppearance">@style/CustomDialogTextAppearance</item>
    </style>

    <style name="CustomDialogTextAppearance">
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
    </style>


</resources>
